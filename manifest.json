{  
  "name": "No advertising",  
  "manifest_version": 2,
  "version": "0.9.0",  
  "description": "No advertising!!!",  
  "background": { "scripts": ["background.js"] },
  "content_scripts": [
    {
      "matches": ["https://www.baidu.com/*"],
      "js": ["jquery.js", "myscript.js"]
    }
	],
  "browser_action": {  
    "default_icon": "icon.png" ,
    "default_title": "No advertising",
    "default_popup": "popup.html"
  }  
} 


